<div class="container mt-5">
  <h1 class="profile-heading mb-5"><span><%= User.find(params[:id]).email %>視聴ログ一覧</span></h1>
  <table class="table text-center">
    <thead class="thead-dark">
      <tr>
        <th>ID</th>
        <th>視聴したページ</th>
        <th>視聴した時刻</th>
      </tr>
    </thead>
    <% @logs.each do |log| %>
      <tr>
        <td><%= log.id %></td>
        <td><%= log.landing_on %></td>
        <td><%= log.landing_at %></td>
      </tr>
    <% end %>
  </table>
  <%= link_to 'ブログ一覧', admin_blogs_path, class: 'btn btn-dark btn-lg' %>
</div>
